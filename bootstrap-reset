#/usr/bash
rm -frv ${HOME}.base16_theme 
rm -frv ${HOME}.bash-git-prompt
rm -frv ${HOME}.bash_history
rm -frv ${HOME}.bash_profile 
rm -frv ${HOME}.bash_sessions
rm -frv ${HOME}.bashrc 
rm -frv ${HOME}.cache
rm -frv ${HOME}.config.vim 
rm -frv ${HOME}.git-credentials
rm -frv ${HOME}.gitconfig 
rm -frv ${HOME}.gitignore_global 
rm -frv ${HOME}.gvimrc 
rm -frv ${HOME}.inputrc 
rm -frv ${HOME}.mutt 
rm -frv ${HOME}.muttrc 
rm -frv ${HOME}.ssh 
rm -frv ${HOME}.tmux 
rm -frv ${HOME}.tmux.conf 
rm -frv ${HOME}.vim 
rm -frv ${HOME}.viminfo
rm -frv ${HOME}.vimrc 
rm -frv ${HOME}.vimrc_background 
rm -frv ~/git
ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/uninstall)"
